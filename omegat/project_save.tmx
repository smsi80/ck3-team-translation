    </tu>
    <tu>
      <tuv lang="en">
        <seg>"Majesty Focus"</seg>
      </tuv>
      <tuv lang="fa" changeid="mohammad" changedate="20200921T183856Z" creationid="mohammad" creationdate="20200921T183856Z">
